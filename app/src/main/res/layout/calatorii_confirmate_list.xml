<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/linearLayoutTitle">

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/cauta_layout_shape_8" >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="72dp"
            android:background="@drawable/cauta_layout_shape_8"
            android:gravity="center_vertical" >

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="3dp"
                android:layout_marginLeft="6dp"
                android:id="@+id/linearLayout5"
                android:background="@drawable/cauta_layout_shape_8" >

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/login_account"
                    android:id="@+id/imageViewProfil" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:layout_marginTop="8dp"
                android:layout_marginRight="120dp"
                android:layout_marginLeft="10dp" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Calatoria 55"
                    android:textColor="@color/colorWhite"
                    android:textSize="17dp"
                    android:textStyle="bold"
                    android:id="@+id/textViewNumarCalatorieConfirmate" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Burada Silviu"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    android:layout_marginTop="2dp"
                    android:textColor="@color/colorWhite"
                    android:id="@+id/textViewNume" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="-90dp"
            android:layout_marginTop="-1dp"
            android:paddingRight="20dp"
            android:paddingLeft="5dp"
            android:paddingBottom="9dp"
            android:paddingTop="18dp"
            android:background="@drawable/cauta_layout_shape_8" >

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingBottom="7dp"
                android:layout_marginLeft="5dp"
                android:layout_marginBottom="5dp" >

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/calatorii_confirmate_pasageri"
                    android:id="@+id/imageView11" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:gravity="right" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="2"
                        android:textSize="16dp"
                        android:paddingLeft="5dp"
                        android:paddingRight="5dp"
                        android:background="@drawable/cauta_layout_shape_6"
                        android:textStyle="bold"
                        android:layout_marginTop="3dp"
                        android:textColor="#53a6ff"
                        android:id="@+id/textViewAltiPasageri" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="9dp"
        android:layout_marginTop="-3dp"
        android:background="@drawable/cauta_layout_shape_6">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="4dp"
            android:paddingTop="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="6dp"
            android:background="@drawable/calatorii_adaugate_shape_textview" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Punct plecare"
                android:textSize="15dp"
                android:textColor="@color/colorGrayDarker"
                android:id="@+id/textView71" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Bucuresti"
                android:textSize="16dp"
                android:layout_gravity="right"
                android:gravity="right"
                android:textStyle="bold"
                android:textColor="@color/colorBlue"
                android:layout_marginLeft="7dp"
                android:id="@+id/textViewPunctPlecare" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="4dp"
            android:paddingTop="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="6dp"
            android:background="@drawable/calatorii_adaugate_shape_textview" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Punct sosire"
                android:textSize="15dp"
                android:textColor="@color/colorGrayDarker"
                android:id="@+id/textView55" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Turnu Magurele,Teleorman"
                android:textSize="16dp"
                android:layout_gravity="right"
                android:gravity="right"
                android:textStyle="bold"
                android:textColor="@color/colorBlue"
                android:layout_marginLeft="7dp"
                android:id="@+id/textViewPunctSosire" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="4dp"
            android:paddingTop="3dp"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="3dp"
            android:background="@drawable/calatorii_adaugate_shape_textview" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Pret"
                android:textSize="15dp"
                android:textColor="@color/colorGrayDarker"
                android:id="@+id/textView27" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:gravity="right" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="10 lei"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:textColor="@color/colorBlue"
                    android:id="@+id/textViewPret" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="4dp"
            android:paddingTop="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="6dp"
            android:background="@drawable/calatorii_adaugate_shape_textview" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Data"
                android:textSize="15dp"
                android:textColor="@color/colorGrayDarker"
                android:id="@+id/textView57" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="2016.07.08"
                android:textSize="16dp"
                android:layout_gravity="right"
                android:gravity="right"
                android:textStyle="bold"
                android:textColor="@color/colorBlue"
                android:layout_marginLeft="7dp"
                android:id="@+id/textViewData" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingBottom="8dp"
            android:paddingTop="7dp"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="2dp"
            android:id="@+id/linearLayoutDetalii"
            android:background="@drawable/calatorii_adaugate_shape_textview" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:textSize="16dp"
                android:textColor="#3d9bff"
                android:textStyle="bold"
                android:layout_marginRight="10dp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Mai multe detalii despre calatorie"
                android:id="@+id/textView61" />

            <ImageView
                android:layout_width="25dp"
                android:layout_height="40dp"
                android:background="@drawable/right_arrow"
                android:id="@+id/imageView9" />
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="-2dp"
        android:background="@drawable/calatorie_detalii_shadow">


    </LinearLayout>

</LinearLayout>